{
    "root": true,
    "env": {
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "espree",
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {},
    "globals": {
        "defineProps": "readonly",
        "defineEmits": "readonly",
        "defineExpose": "readonly",
        "withDefaults": "readonly"
    }
}